{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": { "*": [ "types/*" ] },
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2017"
    ],
    "outDir": "../wwwroot/dist",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "es6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "sourceMap": true,
  "include": [
    "custom-typings",
    "src"
  ]
}